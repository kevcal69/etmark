{
  "name": "etmark",
  "version": "1.0.0",
  "description": "A simple app for markdown documents",
  "author": "Kevin E. Calingacion ",
  "private": true,
  "scripts": {
    "start": "npm run build:watch",
    "build:watch": "webpack --progress --watch",
    "build:prod": "webpack --progress --config webpack.prod.js"
  },
  "dependencies": {
    "antv": "^0.2.2",
    "autosize": "^4.0.2",
    "axios": "^0.18.0",
    "marked": "^0.4.0",
    "vue": "^2.5.16",
    "vue-contenteditable-directive": "^1.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "es2015",
      "stage-0"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-url": {},
      "autoprefixer": {}
    }
  }
}
